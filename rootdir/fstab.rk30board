# Android fstab file.

# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#<src>                               <mnt_point>    <type>    <mnt_flags>                                               <fs_mgr_flags>

/dev/block/mtd/by-name/system        /system        ext4      ro,noatime,noauto_da_alloc                                wait
/dev/block/mtd/by-name/cache         /cache         ext4      noatime,nosuid,nodev,nomblk_io_submit,errors=panic        wait
/dev/block/mtd/by-name/userdata      /data          ext4      noatime,nosuid,nodev,nomblk_io_submit,errors=panic        wait,encryptable=/dev/block/mtd/by-name/metadata
